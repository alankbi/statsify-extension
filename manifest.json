{
    "manifest_version": 2,
    "name": "Statsify",
    "description": "Visualize cool stats about a page with a single click!",
    "version": "1.00",
    "icons": {
        "128": "assets/logo.png"
    },
    "browser_action": {
        "default_icon": "assets/logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}